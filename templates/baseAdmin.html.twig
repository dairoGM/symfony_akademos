                    exportOptions: {
                        columns: ':visible',
                        search: 'applied',
                        order: 'applied'
                    },
                    customize: function(xlsx) {
                        var sheet = xlsx.xl.worksheets['sheet1.xml'];
                        var sheetData = $('sheetData', sheet);

                        // 游댳 Obtener los encabezados directamente de la tabla
                        var colHeaders = [];
                        $('.dataTable thead tr:eq(0) th').each(function() {
                            colHeaders.push($(this).text().trim());
                        });

                        // 游댳 Crear una nueva fila con los encabezados en negrita
                        var newRow = '<row>';
                        for (var i = 0; i < colHeaders.length; i++) {
                            newRow += '<c t="inlineStr" s="2"><is><t>' + colHeaders[i] + '</t></is></c>';
                        }
                        newRow += '</row>';

                        // 游댳 Insertar la nueva fila al principio del Excel
                        sheetData.prepend(newRow);

                        // 游댳 Agregar estilo en negrita a los encabezados
                        var styles = xlsx.xl['styles.xml'];
                        var cellXfs = $('cellXfs', styles);
                        var fonts = $('fonts', styles);

                        // Agregar una nueva fuente en negrita
                        fonts.append('<font><b/><sz val="12"/></font>');
