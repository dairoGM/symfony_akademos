<div class="row">
    <div class="col-6">
        <h5 class="mb-0"><b>Curso académico:</b> {{ item.cursoAcademico.nombre | default('') }}</h5>
        <h5 class="mb-0"><b>Año de aprobación:</b> {{ item.annoAprobacion | default('') }}</h5>
        <h5 class="mb-0"><b>AOCE de formación:</b> {{ item.oace.nombre | default('') }}</h5>
        <h5 class="mb-0"><b>Plan de estudio: </b> <a target="_blank"
                                                href="{{ app.request.getSchemeAndHttpHost() }}/uploads/pregrado/plan_estudio/{{ item.planEstudio }}">{{ item.planEstudio }}</a>
        </h5>
        <h5 class="mb-0"><b>Duración del Curso Por Encuentro:</b> {{ item.duracionCursoPorEncuentro  | default('0') }} años</h5>
    </div>
    <div class="col-6">
        <h5 class="mb-0"><b>Carrera:</b> {{ item.carrera.nombre | default('') }}</h5>
        <h5 class="mb-0"><b>Fecha de aprobación:</b> {{ item.fechaAprobacion | date('d/m/Y') }}</h5>
        <h5 class="mb-0"><b>Organismo demandante:</b> {{ item.organismoDemandante.nombre | default('') }}</h5>
        <h5 class="mb-0"><b>Duración del Curso Diurno:</b> {{ item.duracionCursoDiurno | default('0') }} años</h5>
        <h5 class="mb-0"><b>Duración del Curso a Distancia:</b> {{ item.duracionCursoDistancia | default('0') }} años</h5>
    </div>
</div>


