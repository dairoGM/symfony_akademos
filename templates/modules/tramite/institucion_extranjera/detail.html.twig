<div class="row">
    <div class="col-6">
        <h5 class="mb-0"><b>Nombre:</b> {{ item.nombre }}</h5>
    </div>
    <div class="col-6">
        <h5 class="mb-0"><b>Siglas:</b> {{ item.siglas }}</h5>
    </div>
    <div class="col-6">
        <h5 class="mb-0"><b>País:</b> {{ item.pais.nombre }}</h5>
    </div>
    <div class="col-6">
        <h5 class="mb-0"><b>Provincia:</b> {{ item.provincia }}</h5>
    </div>
    <div class="col-6">
        <h5 class="mb-0"><b>Correo:</b> {{ item.correo }}</h5>
    </div>
    <div class="col-6">
        <h5 class="mb-0"><b>Teléfono:</b> {{ item.telefono }}</h5>
    </div>
    <div class="col-6">
        <h5 class="mb-0"><b>Sitio web:</b> {{ item.sitioWeb }}</h5>
    </div>
    <div class="col-6">
        {% if '1' == item.activo %}
            <h5 class="mb-0"><b>Estado:</b> Habilitado</h5>
        {% else %}
            <h5 class="mb-0"><b>Estado:</b> Deshabilitado</h5>
        {% endif %}
    </div>
</div>


<div class="row">
    <div class="col-6">
        <h5 class="mb-0"><b>Descripción:</b> {{ item.descripcion }}</h5>
    </div>
</div>

